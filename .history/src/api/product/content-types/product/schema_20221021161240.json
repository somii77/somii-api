{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "deleted": {
      "type": "boolean",
      "default": false
    },
    "deleted_at": {
      "type": "datetime"
    },
    "product_pricings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-pricing.product-pricing",
      "mappedBy": "product"
    },
    "plans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::plan.plan",
      "mappedBy": "product"
    },
    "invoice_line_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice-line-item.invoice-line-item",
      "mappedBy": "product"
    }
  }
}
